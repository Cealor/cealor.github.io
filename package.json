{
  "name": "scottdejonge",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/scottdejonge/scottdejonge"
  },
  "dependencies": {
    "bigfishtv-turret": "^3.1.0"
  },
  "devDependencies": {
    "less": "^2.5.1",
    "watch": "^0.13.0",
    "browser-sync": "^2.7.13",
    "less-plugin-autoprefix": "^1.5.0",
    "less-plugin-clean-css": "^1.5.1"
  },
  "scripts": {
    "sync": "browser-sync start --proxy \"dev.scottdejonge\" --files \"dist/*.css\"",
    "watch": "watch \"npm run build\" \"styles\"",
    "build": "lessc --include-path=\"node_modules/bigfishtv-turret\" styles/styles.less --clean-css --autoprefix  > dist/main.css"
  }
}
