<a
  class="item"
  href="{% if item.page.url %}{{ item.page.url }}{% elif item.url %}{{ item.url }}{% endif %}"
>
  <h3>
    {% if item.data.title %}
      {{ item.data.title }}
    {% elif item.title %}
      {{ item.title }}
    {% endif %}
  </h3>
  <p class="description">
    {% if item.data.description %}
      {{ item.data.description }}
    {% elif item.description %}
      {{ item.description }}
    {% endif %}
  </p>
</a>
